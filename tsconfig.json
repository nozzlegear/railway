{
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "declaration": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "strict": true /* Enable all strict type-checking options. */,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "outDir": "./lib"
    },
    "include": ["src"],
    "exclude": ["src/**/*.spec.ts"]
}
